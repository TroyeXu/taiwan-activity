# 地圖服務配置 (已使用 Leaflet + OpenStreetMap，無需 API Key)
# GOOGLE_MAPS_KEY=your_google_maps_api_key_here  # 已移除
# MAPBOX_ACCESS_TOKEN=your_mapbox_token_here     # 可選

# Claude API (用於資料驗證)
CLAUDE_API_KEY=your_claude_api_key_here

# 資料庫配置
DATABASE_URL=./data/tourism.db

# 應用程式設定
NUXT_PUBLIC_APP_URL=http://localhost:3000
NUXT_PUBLIC_APP_NAME=Taiwan Tourism Map

# 可選的分析和監控
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
SENTRY_DSN=your_sentry_dsn_here

# 其他地圖服務 API Keys (可選)
# MAPBOX_ACCESS_TOKEN=your_mapbox_token_here
# HERE_API_KEY=your_here_api_key_here
# MAPTILER_API_KEY=your_maptiler_api_key_here

# 圖片服務 (可選)
CLOUDINARY_BASE_URL=https://res.cloudinary.com/your-account

# 開發環境設定
NODE_ENV=development
PORT=3000